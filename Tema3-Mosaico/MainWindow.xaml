<Window x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="450" Width="800" Icon="assets/125.ico">
    <DockPanel LastChildFill="True">
        <TextBlock Height="100" DockPanel.Dock="Top" Background="Purple"></TextBlock>
        <TextBlock Width="100" DockPanel.Dock="Left" Background="Purple"></TextBlock>
        <TextBlock Height="100" DockPanel.Dock="Bottom" Background="Green"></TextBlock>
        <TextBlock Width="100" DockPanel.Dock="Right" Background="Green"></TextBlock>
        <DockPanel>
            <TextBlock Height="100" DockPanel.Dock="Bottom" Background="Orange"></TextBlock>
            <TextBlock Width="100" DockPanel.Dock="Left" Background="Orange"></TextBlock>
            <TextBlock Width="100" DockPanel.Dock="Right" Background="DeepPink"></TextBlock>
            <TextBlock Height="100" DockPanel.Dock="Top" Background="DeepPink"></TextBlock>
            <TextBlock Background="Black"></TextBlock>
        </DockPanel>
    </DockPanel>
</Window>
